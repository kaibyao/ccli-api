{
  "name": "ccli-api",
  "version": "0.0.1",
  "description": "Open source API wrapper around CCLI Services",
  "main": "dist/index.js",
  "scripts": {
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "prettier": "npm run prettier:fix",
    "prettier:fix": "prettier --single-quote --trailing-comma es5 --write \"{src,__{tests,mocks}__}/**/*.{js,ts}\"",
    "prettier:test": "prettier --single-quote --trailing-comma es5 --list-different \"{src,__{tests,mocks}__}/**/*.{js,ts}\"",
    "start": "npm run lint && npm test && tsc -w",
    "test": "jest"
  },
  "keywords": [
    "ccli",
    "api",
    "songselect"
  ],
  "author": "Kai Yao",
  "license": "MIT",
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testEnvironment": "node",
    "transform": {
      "^.+\\.(ts)$": "<rootDir>/__tests/preprocessor.js"
    },
    "testMatch": [
      "**/__tests__/*.test.(ts|js)",
      "**/?(*.)test.(ts|js)"
    ]
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.2",
    "express": "^4.15.3"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.2",
    "@types/express": "^4.0.36",
    "@types/jest": "^20.0.2",
    "@types/supertest": "^2.0.1",
    "jest": "^20.0.4",
    "prettier": "^1.5.2",
    "supertest": "^3.0.0",
    "tslint": "^5.5.0",
    "typescript": "^2.4.1"
  }
}
